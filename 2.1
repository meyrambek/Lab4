<script>
A= Number(prompt("a san:"," "));
B= Number(prompt("b san:"," "));
xor=mxor(A,B);
 function mxor() {
  if (A==1 && B==1) {
  	return 0;
  } else if(A==1 && B==0){
  	return 1;
  } else if (A==0 && B==0) {
    return 0;
  } else if (A==0 && B==1){
    return 1;
  }
}
  console.log("xor= ",xor);
 
</script>
