<script>
A= Number(prompt("a san:"," "));
B= Number(prompt("b san:"," "));
C= Number(prompt("c san:"," "));
xor=mxor(A,B,C);
 function mxor() {
  if (A==1 && B==1&& C==1) {
  	return 1;
  } else if(A==1 && B==1 && C==0){
  	return 1;
  } else if (A==1 && B==0 && C==0) {
    return 1;
     } else if (A==0 && B==0 && C==0) {
    return 0;
} else if(A==0 && B==0 && C==1){
  	return 0;
  } else if (A==1 && B==0 && C==1){
    return 1;
 } else if (A==0 && B==0 && C==1){
    return 1;
    } else if (A==0 && B==1 && C==0){
    return 1;
  }
}
  console.log("xor= ",xor);
 
</script>
